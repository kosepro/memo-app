{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],

    "rules": {
        "indent": 0,
        "react/jsx-filename-extension": 0,
        "react/destructuring-assignment": 0,
        "react/prefer-stateless-function": 0,
        "react/self-closing-comp": 0,
        "react/jsx-indent": 0,
        "react/prop-types": 0,
        "no-use-before-define": 0,
        "no-undef": 0,
        "no-unused-vars": 0,
        "no-trailing-spaces": 0
    },
    "parser": "babel-eslint"
}
