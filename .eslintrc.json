{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],

    "rules": {
        "react/jsx-filename-extension": 0,
        "no-use-before-define": 0,
        "indent": [1, 4],
        "react/prefer-stateless-function": 0,
        "react/jsx-indent": 0,
        "no-undef": 0,
        "no-unused-vars": 0
    },
    "parser": "babel-eslint"
}
